# Frontend Environment Variables
# Copy this to frontend/.env.local and update as needed

# API Configuration
VITE_API_BASE_URL=http://127.0.0.1:8000
VITE_API_URL=http://127.0.0.1:8000/api
VITE_WS_URL=ws://127.0.0.1:8000

# Application Configuration
VITE_APP_NAME=Natpudan AI Medical Assistant
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_CHAT_STREAMING=true
VITE_ENABLE_KNOWLEDGE_BASE=true
VITE_ENABLE_DRUG_CHECKER=true
VITE_ENABLE_DIAGNOSIS=true

# Analytics (Optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Development
VITE_DEBUG=false
VITE_LOG_LEVEL=info
